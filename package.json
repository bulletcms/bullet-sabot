{
  "name": "bullet-sabot",
  "version": "0.1.0",
  "description": "a web server for bullet-cms",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "webpack --watch -d --progress --colors",
    "build": "webpack -p --optimize-dedupe --progress --colors",
    "start": "nodemon --harmony app.js",
    "dev": "concurrently -kr 'npm run build-dev' 'npm run start'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bulletcms/bullet-sabot.git"
  },
  "author": "xorkevin",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/bulletcms/bullet-sabot/issues"
  },
  "homepage": "https://github.com/bulletcms/bullet-sabot#readme",
  "dependencies": {
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "concurrently": "^2.2.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.10.0",
    "webpack": "^1.13.1",
    "webpack-node-externals": "^1.3.3"
  }
